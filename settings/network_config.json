{
    "model":{
        "name": "pregis_net",
        "n_of_feature": 32,
        "activation_function": "LeakyReLU",
        "bn": false,
        "dp": false,
        "dp_p": 0.2,
        "dataset": "pseudo_2D",
        "pregis_net":{
            "momentum_net": {
                "name": "MomentumNet"
            },
            "recons_net": {
                "name": "VaeNet",
                "gamma_recons": 1,
                "use_TV_loss": true
            },
            "pretrain_epochs": 500,
            "join_two_networks": false
        }
    },
    "train":{
        "batch_size":20,
        "optimizer":{
            "name": "ADAM",
            "lr": 0.001,
            "scheduler":{
                "name": "multistepLR",
                "milestones": [
                   1500,
                   3000,
                   5000
                ],
                "gamma": 0.1
            }
        }
    },
    "validate":{
        "batch_size":20,
        "validate_interval": 20
    },
    "test":{
    }
}
