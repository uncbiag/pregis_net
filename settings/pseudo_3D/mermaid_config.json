{
    "model": {
        "deformation": {
            "compute_similarity_measure_at_low_res": false,
            "map_low_res_factor": 0.5,
            "use_map": true
        },
        "registration_model": {
            "forward_model": {
                "adjoin_on": true,
                "atol": 1e-05,
                "number_of_time_steps": 10,
                "rtol": 1e-05,
                "smoother": {
                    "multi_gaussian_stds": [
                        0.05,
                        0.1,
                        0.15,
                        0.2,
                        0.25
                    ],
                    "multi_gaussian_weights": [
                        0.06666666666666667,
                        0.13333333333333333,
                        0.19999999999999998,
                        0.26666666666666666,
                        0.3333333333333333
                    ],
                    "type": "multiGaussian"
                },
                "smoother_for_forward": {
                    "multi_gaussian_stds": [
                        0.05,
                        0.1,
                        0.15,
                        0.2,
                        0.25
                    ],
                    "multi_gaussian_weights": [
                        0.06666666666666667,
                        0.13333333333333333,
                        0.19999999999999998,
                        0.26666666666666666,
                        0.3333333333333333
                    ],
                    "type": "multiGaussian"
                },
                "solver": "rk4"
            },
            "load_velocity_from_forward_model": false,
            "loss": {
                "display_max_displacement": false,
                "limit_displacement": false,
                "max_displacement": 0.1
            },
            "similarity_measure": {
                "develop_mod_on": false,
                "lncc": {
                    "resol_bound":[128,64],
                    "kernel_size_ratio":[[0.0625,0.125, 0.25], [0.25,0.5], [0.5]],
                    "kernel_weight_ratio":[[0.1, 0.3, 0.6],[0.3,0.7],[1.0]],
                    "stride":[[0.25,0.25,0.25],[0.25,0.25],[0.25]],
                    "dilation":[[2,2,2],[2,2],[1]]
                },
                "sigma": 2.236,
                "type": "lncc"
            },
            "spline_order": 1,
            "type": "svf_vector_momentum_map",
            "use_CFL_clamping": true
        }
    },
    "optimizer": {
        "gradient_clipping": {
            "clip_display": true,
            "clip_individual_gradient": true,
            "clip_individual_gradient_value": 1,
            "clip_shared_gradient": false,
            "clip_shared_gradient_value": 1.0
        },
        "name": "sgd",
        "scheduler": {
            "factor": 0.5,
            "patience": 10,
            "verbose": true
        },
        "sgd": {
            "individual": {
                "dampening": 0.0,
                "lr": 0.025,
                "momentum": 0.9,
                "nesterov": true,
                "weight_decay": 0.0
            },
            "shared": {
                "dampening": 0.0,
                "lr": 0.025,
                "momentum": 0.9,
                "nesterov": true,
                "weight_decay": 0.0
            }
        },
        "single_scale": {
            "nr_of_iterations": 200,
            "rel_ftol": 1e-17
        },
        "use_step_size_scheduler": true,
        "weight_clipping_type": "None",
        "weight_clipping_value": 1.0
    }
}
